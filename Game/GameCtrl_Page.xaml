<Page x:Class="Hosam_App.Game.GameCtrl_Page"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Hosam_App"
        mc:Ignorable="d" d:DesignHeight="900"
        d:DesignWidth="1200" Title="adjust_Ctrl">

    <Grid Background="White">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1.5*" />
            <ColumnDefinition Width="7*" />
        </Grid.ColumnDefinitions>
        <Frame x:Name="Game_Navigation" Grid.Column="0"
                Source="Game_Navigation.xaml"
                NavigationUIVisibility="Hidden" />
        <Frame x:Name="Game_Page" Grid.Column="1"
                Source="Game_Library.xaml"
                NavigationUIVisibility="Hidden" />

    </Grid>
</Page>
